{
  "name": "letteroids",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "build": "browserify src/client/js/main.js -t babelify --outfile src/client/js/bundle.js",
    "lint": "eslint src/",
    "test": "mocha --compilers js:babel-register --recursive test/",
    "clean": "rm export/ -rf",
    "makedirs": "mkdir -p export/js/",
    "minify": "uglifyjs --compress --output export/js/bundle.js -- src/client/js/bundle.js",
    "copyfiles": "cp -r src/client/css src/client/index.html export/",
    "export": "NODE_ENV=production npm run makedirs && npm run build && npm run minify && npm run copyfiles"
  },
  "author": "prvak",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "chai-shallow-deep-equal": "^1.4.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "flux": "^2.1.1",
    "immutable": "^3.8.1",
    "mocha": "^2.4.5",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "seedrandom": "^2.4.2",
    "uglify-js": "^2.6.2"
  }
}
